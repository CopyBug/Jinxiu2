<?xml version="1.0" encoding="utf-8"?>
<layout>
<data>
    <variable
        name="Msearchword"
        type="com.lhw.sion.bean.DataBingSearchAdapter" />
</data>
    <LinearLayout
        android:background="@color/blue03"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:padding="10dp"
            android:paddingLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="@color/blue06"
                    android:layout_width="wrap_content"
                    android:text="@{Msearchword.word}"
                    android:textSize="@dimen/themmargin"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:paddingLeft="10dp"
                    android:layout_width="wrap_content"
                    android:text="@{Msearchword.britishStandard}"
                    android:textColor="@color/color_text2_black_grace"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:text="@{Msearchword.transaction}"
                android:textColor="@color/color_text2_black_grace"
                android:layout_height="wrap_content"/>
        </LinearLayout>

    </LinearLayout>
</layout>