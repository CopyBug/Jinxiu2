<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:myView="http://schemas.android.com/apk/res-auto">
<data>
    <variable
        name="bean"
        type="com.lhw.sion.bean.DatabingLearnAdapter" />
</data>
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:gravity="center"
        android:layout_height="wrap_content"

        >
       <LinearLayout
           android:layout_margin="10dp"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:gravity="center">
           <com.lhw.sion.ui.view.LearnLayout
               android:binstars="@{bean.star}"
                android:mtextview="@{String.valueOf(bean.number)}"
               android:layout_width="70dp"
               android:layout_height="70dp"/>
       </LinearLayout>
    </LinearLayout>
</layout>